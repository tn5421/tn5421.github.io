function actionName(name)
	{
	if(name)
		document.getElementById("actionText").value = name;
	}
	
function changePage(pid)
	{
	if(pid)
		{
		if(pid.charAt(0) == "!")
			{
			switch(pid)
				{
				case "!sidebar":
					colapseSide();
					break;
				case "!autofocus":
					toggleFocus();
					break;
				case "!scavenge": 
					document.getElementById("findFood").submit();
					break;
				case "!core2layout": 
					document.getElementById("layout").value = "core2";
					document.getElementById("layoutForm").submit();
					break;
				case "!defaultlayout": 
					document.getElementById("layout").value = "default";
					document.getElementById("layoutForm").submit();
					break;
				case "!lightlayout": 
					document.getElementById("layout").value = "light";
					document.getElementById("layoutForm").submit();
					break;
				case "!mobilelayout": 
					document.getElementById("layout").value = "mobile";
					document.getElementById("layoutForm").submit();
					break;
				case "!dmission": 
					document.getElementById("DmissionForm").submit();
					break;
				case "!cmission": 
					document.getElementById("CmissionForm").submit();
					break;
				case "!bmission": 
					document.getElementById("BmissionForm").submit();
					break;
				case "!amission": 
					document.getElementById("AmissionForm").submit();
					break;
				case "!ccrime": 
					document.getElementById("CcrimeForm").submit();
					break;
				case "!bcrime": 
					break;
				case "!acrime": 
					break;
				default:
					alert(pid);
					break;
				}
			}
		else
			document.getElementById("tnr").src = pid;
		}
	}
	
function manualCallback(called)
	{
	callTimer = setInterval(function(){checkTimers();},1);
	function checkTimers()
	{
	if($.timers.length ==  0)
		{
		clearInterval(callTimer);
		called();
		}
	}
	}

function colapseSide()
	{
	if(sideAlign == "left")
		{
		if(sideOut == 1)
			{
			$("#sideContainer").animate({width: '0'});
			$(".invisible").css("visibility","hidden");
			$(".pleaseHide").css("display","none");
			if(document.getElementById("hideKeyCheck").checked)
				{
				$("#showHide").css("display","none");
				}
			$("#showHide").css("left","-150px");
			$("#container").animate({left: '0'}, function()
				{
				$("#sideBar").css("overflow","visible");
				$("#showHide").css("left","-110px");
				});
			sideOut = 0;	
			}
		else if(sideOut == 0)
			{
			$("#sideContainer").animate({width: '200px'});
			$("#showHide").css("left","0");
			$(".invisible").css("visibility","visible");
			$("#container").animate({left: '200px'}, function()
				{
				$("#sideBar").css("overflow-x","hidden");
				$("#sideBar").css("overflow-y","scroll");
				});
			if(document.getElementById("hideKeyCheck").checked)
				{
				$("#showHide").css("display","block");
				}
			sideOut = 1;
			}
		}
	else if(sideAlign == "right")
		{
		if(sideOut == 1)
			{
			$("#sideContainer").animate({width: '0'});
			$(".pleaseHide").css("display","none");
			if(document.getElementById("hideKeyCheck").checked)
				{
				$("#showHide").css("display","none");
				}
			$("#showHide").css("right","-150px");
			$("#container").animate({right: '0'}, function()
				{
				$("#sideBar").css("overflow","visible");
				$("#showHide").css("left","-70px");
				});
			sideOut = 0;
			}
		else if(sideOut == 0)
			{
			$("#sideContainer").animate({width: '200px'});
			$("#showHide").css("left","0");
			$("#container").animate({right: '200px'}, function()
				{
				$("#sideBar").css("overflow-x","hidden");
				$("#sideBar").css("overflow-y","scroll");
				});
			if(document.getElementById("hideKeyCheck").checked)
				{
				$("#showHide").css("display","block");
				}
			sideOut = 1;	
			}
		}
	}
	
function showStuff(id)
	{
	var selector = "#" + id;
	$(selector).slideToggle();
	}

function leftSide()
	{
	$("#sideContainer").css({"right":"auto","left":"0"});
	$("#container").css({"left":"200px","right":"0"});
	sideAlign = "left";
	}
	
function rightSide()
	{
	$("#sideContainer").css({"right":"0","left":"auto"});
	$("#container").css({"left":"0","right":"200px"});
	sideAlign = "right";
	}
	
function answerQuestion()
	{
	alert("If the sidebar is hidden while this box is checked," +
	"there will be no tab to unhide the sidebar. Instead you will" +
	"either have to reload the page, or use a key command to unhide it.");
	}
	
function eachTime()
	{
	parent.document.getElementById("actionText").focus();
	}
	
function toggleControls()
	{
	if(controlsOn == 0)
		{
		if(sideOut == 0)
			{
			colapseSide();
			function continueControls()
				{
				$("#tnr").css("display","none");
				$("#sideContainer").css("display","none");
				$("#container").css({"overflow":"visible","left":"0","right":"0"});
				$("#content").css("display","block");
				}
			manualCallback(continueControls);
			sideOut = 0;
			}
		else
			{
			$("#tnr").css("display","none");
			$("#sideContainer").css("display","none");
			$("#container").css({"overflow":"visible","left":"0","right":"0"});
			$("#content").css("display","block");
			}
		if(focusOn == 1)
			{
			clearInterval(interval);
			}
		controlsOn = 1;
		}
	else if(controlsOn == 1)
		{
		if(focusOn == 1)
			{
			interval = setInterval(function(){eachTime()},1);
			}
		$("#tnr").css("display","block");
		$("#sideContainer").css("display","block");
		if(sideAlign == "left")
			{
			$("#container").css({"overflow":"hidden","left":"200px"});
			}
		else if(sideAlign == "right")
			{
			$("#container").css({"overflow":"hidden","right":"200px"});	
			}
		$("#content").css("display","none");
		setControls();
		controlsOn = 0;
		if(!sideOut)
			{
			sideOut = 1;
			colapseSide();
			}
		}
	}
	
function toggleFocus()
	{
	if(controlsOn == 0)
		{
		if(focusOn == 0)
			{
			$("#switch").css("color","#bdbdbd").text("On");
			interval = setInterval(function(){eachTime()},1);
			focusOn = 1;
			}
		else if(focusOn == 1)
			{
			$("#switch").css("color","#000000").text("Off");
			clearInterval(interval);
			focusOn = 0;
			}
		}
	}
	
