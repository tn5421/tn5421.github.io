function doAction(evt)
	{
	if(controlsOn == 0)
		{
		var key = (evt) ? evt.which : event.keyCode;
		switch(key)
			{
			case 13: // Enter
				changePage(alphaIndex[0]);
				actionName(alphaNames[0]);
				break;	
			case 32: // Spacebar
				changePage(alphaIndex[37]);
				actionName(alphaNames[37]);			
			case 37: // Left
				changePage(alphaIndex[38]);
				actionName(alphaNames[38]);
				break;
			case 38: // Up
				changePage(alphaIndex[39]);
				actionName(alphaNames[39]);
				break;
			case 39: // Right
				changePage(alphaIndex[40]);
				actionName(alphaNames[40]);
				break;
			case 40: // Down
				changePage(alphaIndex[41]);
				actionName(alphaNames[41]);
				break;
