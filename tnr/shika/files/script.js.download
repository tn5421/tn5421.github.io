function updateValues(box)
	{
	var test = box.value;
	box.value = test.toUpperCase();
	var test = box.value;
	if(test != "")
		{
		var i = 1;
		while(i < 449)
			{
			var compare = document.getElementById(i);
			if(compare.value == test)
				{
				if(compare.id != box.id)
					{
					compare.value = "";
					}
				}
			i++;
			}
		}
	}

function setChars(id,evt)
	{
	var key = (evt) ? evt.which : event.keyCode;
	id.value = "";
	id.maxLength = "5";
	switch(key)
		{
		case 13: // Enter
			id.value = "ENTER";
			break;
		case 32: // Space
			id.value = "SPACE";
			break;
		case 37: // Left
			id.value = "LEFT";
			break;
		case 38: // Up
			id.value = "UP";
			break;
		case 39: // Right
			id.value = "RIGHT";
			break;
		case 40: // Down
			id.value = "DOWN";
			break;
		default:
			break;
		}
	}
	
function saveSettings()
	{
	var cookieText = "UNBsettings=";
	if(sideAlign == "left")
		cookieText += "1!";
	else
		cookieText += "0!";
	if(document.getElementById("hideKeyCheck").checked)
		cookieText += "1!";
	else
		cookieText += "0!";
	if(document.getElementById("bankCheck").checked)
		cookieText += "1";
	else
		cookieText += "0";
	if(document.getElementById("trainCheck").checked)
		cookieText += "1";
	else
		cookieText += "0";
	if(document.getElementById("errandsCheck").checked)
		cookieText += "1";
	else
		cookieText += "0";
	if(document.getElementById("travelCheck").checked)
		cookieText += "1";
	else
		cookieText += "0";
	if(document.getElementById("linksCheck").checked)
		cookieText += "1";
	else
		cookieText += "0";
	if(document.getElementById("searchCheck").checked)
		cookieText += "1";
	else
		cookieText += "0";
	if(document.getElementById("settingsCheck").checked)
		cookieText += "1!";
	else
		cookieText += "0!";
	if(document.getElementById("collapseSet").checked)
		cookieText += "1!";
	else
		cookieText += "0!";
	if(document.getElementById("focusOn").checked)
		cookieText += "1!";
	else
		cookieText += "0!";
	var i = 1;
	while(i < 449)
		{
		boxVal = document.getElementById(i).value;
		if(boxVal != "")
			cookieText += boxVal + "!";
		else
			cookieText += "?!";
		i++;
		}
	var date = new Date();
	date.setTime(date.getTime()+(300*24*60*60*1000));
	cookieText = cookieText + "; expires=" + date.toGMTString() + "; path=/";
	document.cookie = cookieText;
	}
	
function loadSettings()
	{
	if(document.cookie)
		{
		var cookieJar = document.cookie.split("!");
		if(cookieJar[0] == "UNBsettings=0")
			rightSide();
		if(cookieJar[1] == 1)
			{
			document.getElementById("hideKeyCheck").checked = true;
			$("#showHide").css("display","none");
			}
		if(cookieJar[2].charAt(0) == 1)
			{
			document.getElementById("bankCheck").checked = true;
			showStuff("banker");
			}
		if(cookieJar[2].charAt(1) == 1)
			{
			document.getElementById("trainCheck").checked = true;
			showStuff("trainForm");
			}
		if(cookieJar[2].charAt(2) == 1)
			{
			document.getElementById("errandsCheck").checked = true;
			showStuff("errandsForm");
			}
		if(cookieJar[2].charAt(3) == 1)
			{
			document.getElementById("travelCheck").checked = true;
			showStuff("travelling");
			}
		if(cookieJar[2].charAt(4) == 1)
			{
			document.getElementById("linksCheck").checked = true;
			showStuff("buttons");
			}
		if(cookieJar[2].charAt(5) == 1)
			{
			document.getElementById("searchCheck").checked = true;
			showStuff("userForm");
			}
		if(cookieJar[2].charAt(6) == 1)
			{
			document.getElementById("settingsCheck").checked = true;
			showStuff("settings");
			}
		if(cookieJar[3] == 1)
			{
			colapseSide();
			document.getElementById("collapseSet").checked = true;
			}
		if(cookieJar[4] == 1)
			{
			toggleFocus();
			var text = document.getElementById("switch");
			document.getElementById("focusOn").checked = true;
			}
		var i = 5;
		var boxID = 1;
		while(boxID < 449)
			{
			if(cookieJar[i] != "?")
				document.getElementById(boxID).value = cookieJar[i];
			else
				document.getElementById(boxID).value = "";
			boxID++;
			i++;
			}
		setControls();
		}
	}
	
